			if(this.username == pokerpg.Game.username) {
				if(this.isControllable() && !this.willMoveIntoAWall() && (this.direction == 1 && !!pokerpg.UI.keysDown[37] || this.direction == 0 && !!pokerpg.UI.keysDown[40] || this.direction == 3 && !!pokerpg.UI.keysDown[39] || this.direction == 2 && !!pokerpg.UI.keysDown[38])) {
