pokerpg.CCharacter.prototype.useLedge = function() {
	var me = this;
	var frontX = this.getFrontPositionX();
	var frontY = this.getFrontPositionY();
	var destX = this.getFrontPositionX(2);
	var destY = this.getFrontPositionY(2);
	this.walking = true;
	this.noclip = true;
	this.transmitWalk = false;
	this.jumping = true;
	if(this.username == pokerpg.Game.username) pokerpg.Game.curGame.playerCanMove = false;
	var tmpCount = 0;
	var renderFunc = null;
	renderFunc = function() {
		++tmpCount;
		if(me.x != destX || me.y != destY) me.walking = true;
		me.renderOffsetY = Math.min(Math.round(8 / 15 * (tmpCount * tmpCount) + -8 * tmpCount),0);
		if(tmpCount == 18) new pokerpg.entities.CLedgeSmoke(me.x,me.y);
		if(tmpCount >= 20) {
			me.renderOffsetY = 0;
			me.walking = false;
			if(me.username == pokerpg.Game.username) pokerpg.Game.curGame.playerCanMove = true;
			me.noclip = false;
			me.jumping = false;
			me.transmitWalk = true;
			me.freezeTicks = 2;
			me.x = destX;
			me.y = destY;
			me.lastX = me.x;
			me.lastY = me.y;
			pokerpg.Renderer.unHookRender(renderFunc);
		}
	};
	if(this.username == pokerpg.Game.username) pokerpg.Connection.socket.emit("useLedge",{ ack : pokerpg.Connection.lastAckMove, x : frontX, y : frontY});
	pokerpg.Renderer.hookRender(renderFunc);
}
pokerpg.CCharacter.prototype.willMoveIntoAWall = function() {
	var posX = this.getFrontPositionX();
	var posY = this.getFrontPositionY();
	var map = pokerpg.Game.curGame.map;
	return map.hasTileProp(posX,posY,"solid") || map.hasTileProp(posX,posY,"water") || map.hasTileProp(posX,posY,"ledge");
}
pokerpg.CCharacter.prototype.getFrontPositionX = function(n) {
	if(n == null) n = 1;
	switch(this.direction) {
	case 1:
		return this.x - n;
	case 3:
		return this.x + n;
	case 2:
		return this.x;
	case 0:
		return this.x;
	}
	return null;
}
pokerpg.CCharacter.prototype.getFrontPositionY = function(n) {
	if(n == null) n = 1;
	switch(this.direction) {
	case 1:
		return this.y;
	case 3:
		return this.y;
	case 2:
		return this.y - n;
	case 0:
		return this.y + n;
	}
	return null;
}
pokerpg.CCharacter.prototype.tickBot = function() {
	if(this.walking) return;
	this.walking = this.x != this.targetX || this.y != this.targetY;
	if(!this.walking) return;
	var lastDirection = this.direction;
	if(Math.abs(this.x - this.targetX) > 0 && this.y == this.targetY) this.direction = this.x < this.targetX?3:1; else if(Math.abs(this.y - this.targetY) > 0 && this.x == this.targetX) this.direction = this.y < this.targetY?0:2; else this.direction = this.targetY < this.y?2:0;
	if(lastDirection != this.direction) this.walkingPerc = 0.0;
}
pokerpg.CCharacter.prototype.render = function(ctx) {
	var me = this;
	if(!this.loaded) return;
	if(this.username == pokerpg.Game.username && !pokerpg.Game.curGame.drawPlayerChar) return;
	var tmpCtx = pokerpg.Main.tmpCtx;
	ctx.save();
