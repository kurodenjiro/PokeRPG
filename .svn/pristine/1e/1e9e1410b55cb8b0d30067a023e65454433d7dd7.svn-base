			destX -= pokerpg.Map.getCurMap().tilewidth * (1 - perc);
			break;
		} else switch(this.direction) {
		case 1:
			destX -= pokerpg.Map.getCurMap().tilewidth * perc;
			break;
		case 3:
			destX += pokerpg.Map.getCurMap().tilewidth * perc;
			break;
