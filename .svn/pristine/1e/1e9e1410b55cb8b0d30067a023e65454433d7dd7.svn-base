						return;
					}
				}
				break;
			case 0:
				if(!!!pokerpg.UI.keysDown[40]) {
					if(this.walkingPerc < MOVE_WAIT) {
						this.walking = false;
						pokerpg.Connection.socket.emit("turn",{ dir : this.direction});
						return;
					}
				}
				break;
			case 3:
				if(!!!pokerpg.UI.keysDown[39]) {
					if(this.walkingPerc < MOVE_WAIT) {
						this.walking = false;
						pokerpg.Connection.socket.emit("turn",{ dir : this.direction});
						return;
					}
				}
				break;
			case 2:
				if(!!!pokerpg.UI.keysDown[38]) {
					if(this.walkingPerc < MOVE_WAIT) {
						this.walking = false;
						pokerpg.Connection.socket.emit("turn",{ dir : this.direction});
						return;
					}
				}
				break;
			}
		}
		if(pokerpg.UI.uiBButtonDown == true) {
			this.walkingPerc += 0.14;
			this.animationStep += 0.30;
