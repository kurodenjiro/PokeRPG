pokerpg.CCharacter.prototype.enterStairs = function(warp) {
	var me = this;
	var tmpX = this.x;
	var tmpY = this.y;
	var canvas = pokerpg.Main.canvas;
	var ctx = pokerpg.Main.ctx;
