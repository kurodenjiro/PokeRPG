		if(this.animationStep > 4.0) this.animationStep -= 4.0;
		if(this.walkingPerc >= (1.0 - MOVE_WAIT) / 2 && !this.walkingHasMoved) {
			var frontX = this.getFrontPositionX();
			var frontY = this.getFrontPositionY();
			if(this.isControllable() && !this.noclip) {
				var tmpWarp = pokerpg.entities.CWarp.getWarpAt(frontX,frontY);
				if(tmpWarp != null && tmpWarp.canWarp(this)) {
					if(Std["is"](tmpWarp,pokerpg.entities.CDoor)) this.enterDoor(tmpWarp); else if(Std["is"](tmpWarp,pokerpg.entities.CWarpArrow)) this.enterWarpArrow(tmpWarp); else if(Std["is"](tmpWarp,pokerpg.entities.CStairs)) this.enterStairs(tmpWarp);
					return;
				} else if(pokerpg.Map.getCurMap().hasTileProp(frontX,frontY,"ledge") && pokerpg.Map.getCurMap().getLedgeDir(frontX,frontY) == this.direction) this.useLedge(); else if(this.willMoveIntoAWall()) {
					pokerpg.Connection.socket.emit("turn",{ dir : this.direction});
					this.walking = false;
					return;
				}
			} else if(pokerpg.Map.getCurMap().hasTileProp(frontX,frontY,"ledge") && pokerpg.Map.getCurMap().getLedgeDir(frontX,frontY) == this.direction) this.useLedge();
			if(!pokerpg.Game.curGame.inBattle || this.username != pokerpg.Game.username) {
				this.lastX = this.x;
				this.lastY = this.y;
			}
			switch(this.direction) {
			case 1:
				this.x -= 1;
				break;
			case 3:
				this.x += 1;
				break;
			case 2:
				this.y -= 1;
				break;
			case 0:
				this.y += 1;
				break;
			}
			this.lastMoveTick = pokerpg.Renderer.numRTicks;
			this.walkingHasMoved = true;
			if(curMap.hasTileProp(this.x,this.y,"grass")) new pokerpg.entities.CGrassAnimation(this.x,this.y);
			if(this.username == pokerpg.Game.username && this.transmitWalk) pokerpg.Connection.socket.emit("walk",{ ack : pokerpg.Connection.lastAckMove, x : this.x, y : this.y, dir : this.direction});
