pokerpg.CCharacter.prototype.enterDoor = function(door) {
	var me = this;
	var tmpX = this.x;
	var tmpY = this.y;
	var canvas = pokerpg.Main.canvas;
	var ctx = pokerpg.Main.ctx;
	door.open();
	this.walking = false;
	if(this.username == pokerpg.Game.username) {
		pokerpg.Game.curGame.playerCanMove = false;
		pokerpg.Game.curGame.queueLoadMap = true;
	}
	var tmpCount = 0;
	var doorRenderTransition = null;
	doorRenderTransition = function() {
		++tmpCount;
		if(tmpCount < 15) return;
		if(tmpCount == 15) {
			me.walking = true;
			me.noclip = true;
			me.transmitWalk = false;
		}
		me.lastX = tmpX;
		me.lastY = tmpY;
		if(me.username == pokerpg.Game.username) {
			if(tmpCount == 23) pokerpg.Game.curGame.drawPlayerChar = false;
			var perc = pokerpg.Util.clamp((tmpCount - 20) / 10,0,1);
			ctx.fillStyle = "rgba(0,0,0," + perc + ")";
			ctx.fillRect(0,0,canvas.width,canvas.height);
			if(tmpCount == 30) {
				me.noclip = false;
				me.transmitWalk = true;
				pokerpg.Game.curGame.queueLoadMap = false;
				if(pokerpg.Game.curGame.queuedMap != null) pokerpg.Game.loadMap(pokerpg.Game.curGame.queuedMap,pokerpg.Game.curGame.queuedChars);
			}
		} else if(tmpCount == 23) {
			me.destroy();
			pokerpg.Renderer.unHookRender(doorRenderTransition);
		}
	};
	if(this.username == pokerpg.Game.username) pokerpg.Connection.socket.emit("useWarp",{ name : door.name, direction : this.direction});
	pokerpg.Renderer.hookRender(doorRenderTransition);
}
